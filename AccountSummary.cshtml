        $('.topNavbar li').click(function () {

            $('#mandateTable').hide();
            activeAccList.children('ul').children().css({ opacity: 1.0 }).show();
            $('#mandateSwitchInput').prop("checked", false);
            $('#mandateSwitchInput2').prop("checked", false);

            if ($(this).find('a').hasClass("active") == false) {
                $(".topNavbar li a").removeClass("active");
                $(this).find('a').addClass('active');
                refreshActive(this);
            }

            $(this).find('a').addClass('active');
            var currentHtml = $(this).find('a span:last-of-type').html();
            $('.accTypeTitle h3').html(currentHtml);

            $('.accList').each(function () {
                $(this).children('ul').children('li').removeClass('active');
            })

            var elemId = $(this).attr('id');

            switch (elemId) {
                case 'casa':
                    onTabClick('#casaAcc', '#casaAccDetails');
                    break;

                case 'pbc':
                    onTabClick('#pbCard', '#cardAccDetails');
                    break;

                case 'fd':
                    onTabClick('#fixedDepositList', '#depositDetails');
                    break;
                case 'gi':
                    onTabClick('#goldList', '#goldDetails');
                    break;
                case 'lf':
                    onTabClick('#loanList', '#loanDetails');
                    break;
