        function selectListItem(listItem, itemClick) {

            if (itemClick == true) {
                var listId = $(listItem).parent().parent().attr('id');
                // console.log(listId);
            } else {
                var temp = listItem;
                var listId = temp;
                listId = listId.replace("#", "");

                console.log(listId);

            }

            $('.actGrp').each(function () {
                $(this).attr('hidden', true);
            })

            switch (listId) {

                case 'casaAcc':
                    accNumber.html($(listItem).find('.accListTitleAndNum p:last-of-type').html());
                    if ($(listItem).find(".iconBox").hasClass('savingAcc')) {
                        accType.html("Savings");
                        accTypeTitle.html("Savings");
                    } else if ($(listItem).find(".iconBox").hasClass('currentAcc')) {
                        accType.html("Current");
                        accTypeTitle.html("Current");
                    }

                    curBalance.html($(listItem).find('.accBalance p').html());
                    availableBalance.html($(listItem).find('.accBalance p').html());
                    $('.cardBal').html($(listItem).find('.accBalance p').html());
                    $('#CardSettings').attr('hidden', true);
                    $('#firstCard').removeClass('bg-gradient-4');
                    $('#firstCard').addClass('bg-gradient-3');
                    $('#CASAIllustration').attr('hidden', false);
                    $('#FDIllustration').attr('hidden', true);
                    $('#GIIllustration').attr('hidden', true);
                    $('#LFIllustration').attr('hidden', true);

                    $('#IFDIllustration').attr('hidden', true);
                    $('#actGrpCASA').attr('hidden', false);
                    break;

                case 'pbCard':
                    accNumberTitle.html($(listItem).find(".accListTitleAndNum p:nth-of-type(2)").html());
                    accTypeTitle.html($(listItem).find(".accListTitleAndNum p:last-of-type").html());
                    pbcAccNumber.html($(listItem).find(".accListTitleAndNum p:nth-of-type(2)").html());
                    pbcAccType.html($(listItem).find(".accListTitleAndNum p:last-of-type").html());
                    pbcAvailableBal.html($(listItem).find(".accBalance p").html());
                    $('.cardBal').html($(listItem).find('.accBalance p').html());
                    $('#CardSettings').attr('hidden', false);
                    $('#firstCard').removeClass('bg-gradient-3');
                    $('#firstCard').addClass('bg-gradient-4');
                    $('#CASAIllustration').attr('hidden', false);
                    $('#FDIllustration').attr('hidden', true);
                    $('#GIIllustration').attr('hidden', true);
                    $('#IFDIllustration').attr('hidden', true);
                    $('#LFIllustration').attr('hidden', true);

                    $('#actGrpCard').attr('hidden', false);


                    break;

                case 'fixedDepositList':
                    accNumberTitle.html($(listItem).find(".accListTitleAndNum p:last-of-type").html());
                    accTypeTitle.html($(listItem).find(".accListTitleAndNum p:first-of-type").html());
                    depositAccNumber.html($(listItem).find(".accListTitleAndNum p:last-of-type").html());
                    depositAccType.html($(listItem).find(".accListTitleAndNum p:first-of-type").html());
                    depositAvailableBal.html($(listItem).find(".accBalance p").html());
                    $('#CardSettings').attr('hidden', true);
                    $('#CASAIllustration').attr('hidden', true);
                    $('#IFDIllustration').attr('hidden', true);
                    $('#GIIllustration').attr('hidden', true);
                    $('#FDIllustration').attr('hidden', false);
                    $('#LFIllustration').attr('hidden', true);
                    $('#actGrpFD').attr('hidden', false);


                    break;

                case 'goldList':
                    accNameTitle.html("Gold");
                    accNumberTitle.html($(listItem).find(".accListTitleAndNum p:first-of-type").html());
                    accTypeTitle.html("Gold Investment");
                    goldAccNumber.html($(listItem).find(".accListTitleAndNum p:last-of-type").html());
                    goldAccType.html("Gold Investment");
                    $('#goldLastStmtDte .col-4').html($(listItem).find(".statementGold").html());
                    goldCurrBal.html($(listItem).find(".accBalance p").html());
                    $('#CardSettings').attr('hidden', true);
                    $('#CASAIllustration').attr('hidden', true);
                    $('#FDIllustration').attr('hidden', true);
                    $('#IFDIllustration').attr('hidden', true);
                    $('#LFIllustration').attr('hidden', true);
                    $('#GIIllustration').attr('hidden', false);
                    $('#actGrpFD').attr('hidden', false);
                    break;

                case 'loanList':
                    accNameTitle.html("Loan");
                    accNumberTitle.html($(listItem).find(".accListTitleAndNum p:first-of-type").html());
                    accTypeTitle.html("Note No: " + $(listItem).find(".accListTitleAndNum p:last-of-type").html());
                    loanAccNumber.html($(listItem).find(".accListTitleAndNum p:first-of-type").html());
                    loanNoteNum.html($(listItem).find(".accListTitleAndNum p:last-of-type").html());
                    loanAmtDue.html($(listItem).find(".accBalance p").html());
                    $('#CardSettings').attr('hidden', true);
                    $('#CASAIllustration').attr('hidden', true);
                    $('#FDIllustration').attr('hidden', true);
                    $('#IFDIllustration').attr('hidden', true);
                    $('#LFIllustration').attr('hidden', false);
                    $('#GIIllustration').attr('hidden', true);
                    break;

            }

            accNameTitle.html($(listItem).find('.accListTitleAndNum p:first-of-type').html());
            $('#accTypeTitle').html($(listItem).find('.accListTitleAndNum p:first-of-type').html());

        }

        function onTabClick(tabId, tabDetailsId) {
            $('.accList').not(tabId).addClass('d-none');
            $(tabId).removeClass('d-none');
            $('.accDetails').children().not(tabDetailsId).addClass('d-none');
            $(tabDetailsId).removeClass('d-none');
            $(tabId).find('ul li:first-of-type').addClass('active');
            // console.log(tabId);
            selectListItem(tabId, false);
        }
    });




    function refreshListItemActive(activeEl) {
        if ($(activeEl).hasClass("active") == false) {
            $(".accList ul li").removeClass("active");
            $(activeEl).addClass('active');
        }
    }

    function refreshActive(activeEl) {
        $('.topNavbar li a').find('span:last-of-type').addClass('d-none');
