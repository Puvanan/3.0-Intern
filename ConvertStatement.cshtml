<script>
    var oneMin = 60,
        display = $('#time');

    function startTimer(duration, display) {
        var timer = duration, minutes, seconds;

        var timerInterval = setInterval(function () {
            minutes = parseInt(timer / 60, 10);
            seconds = parseInt(timer % 60, 10);

            minutes = minutes < 10 ? "0" + minutes : minutes;
            seconds = seconds < 10 ? "0" + seconds : seconds;

            display.text(minutes + " : " + seconds);

            if (--timer < 0) {
                clearInterval(timerInterval);
                timer = duration;
                $('.reqPacBtn').removeClass('disabled');
                $('.timerText').prop("hidden", true);
            }
        }, 1000);
    }

    function validateInputs() {
        if (($('#choices-multiple-remove-button').val().length > 0 || $("#selectAllSwitch").is(":checked")) && ($("#checkDeclaration").is(":checked"))) {
            $('#firstNext').removeClass('disabled');
        }
        else {
            $('#firstNext').addClass('disabled');
        }
    }

    var isSelectAll = false;
    const allChoices = [];

    $(document).ready(function () {

        $('#authMethod').change(function () {
            var selectedIndex = $(this).children('option:selected').index();
            if (selectedIndex != 0) {
                $(this).parent().removeClass('col-sm-12').addClass('col-sm-9');
                $(this).parent().siblings('.authConfirmBtn').attr('hidden', false);
            }

            var cases = $("#authMethod option:selected").val();
            if (cases == "pac") {
                $("#PAC").removeClass("d-none");
                $("#secure").addClass("d-none");
                $("#noti").addClass("d-none");
                $("#token").addClass("d-none");
            }
            if (cases == "token") {
                $("#token").removeClass("d-none");
                $("#PAC").addClass("d-none");
                $("#secure").addClass("d-none");
                $("#noti").addClass("d-none");
            }
            if (cases == "secure") {
                $("#secure").removeClass("d-none");
                $("#PAC").addClass("d-none");
                $("#token").addClass("d-none");
                $("#noti").addClass("d-none");
            }
            if (cases == "noti") {
                $("#noti").removeClass("d-none");
                $("#PAC").addClass("d-none");
                $("#token").addClass("d-none");
                $("#secure").addClass("d-none");
            }
        });

        $('.authConfirmBtn button').click(function () {
            $(this).parent().siblings().find('#authMethod').attr('disabled', true);
            $(this).parent().siblings('.authField').attr('hidden', false);
            $(this).attr('disabled', true);

        });

        var multipleCancelButton = new Choices('#choices-multiple-remove-button', {
            removeItemButton: true,
            searchEnabled: true,
            searchChoices: true,
            searchResultLimit: 5,
            placeholder: true,
            placeholderValue: "Search or select account(s)",
            // searchPlaceholderValue: null,
            // renderChoiceLimit: 5,
        });

        for (let index = 1; index < $('.choices__list').children().length; index++) {
            allChoices.push($('[data-id="' + index + '"]').data('value'));
        }

        $('.mobileNav').each(function () {
            $(this).removeClass('active');
        })

        $('.nav-link').each(function () {
            $(this).removeClass('active');
        })

        $('.profile').each(function () {
            $(this).addClass('active');
        });

        $('.profileMenu').addClass('active');

        $('#firstNext').on('click', function () {
            $('#secondPage').attr('hidden', false);
            $('#firstPage').attr('hidden', true);
            $('#thirdPage').attr('hidden', true);

            $('#firstNote').attr('hidden', true);

            if (isSelectAll == false) {
                for (let i = 1; i <= $('#choices-multiple-remove-button').val().length; i++) {
                    $('#accountNumbers').append('<div class="col-sm-4 mb-1 flex-wrap"><input type="text" class="form-control" id=' + ("accNumber" + i) + ' disabled></div>');
                    $('#accNumber' + i).val($('#choices-multiple-remove-button').val()[i - 1]);
                }
            }

        });

        $('#choices-multiple-remove-button').change(function () {
            validateInputs();
        });

        $('#checkDeclaration').change(function () {
            validateInputs();
        });

        //reset button
        $('#clearButton1').on('click', function () {
            $('form')[0].reset();
            $(".choices").removeClass('is-disabled');
            $('.choices__list').attr('hidden', false);
            $('.choices__input').attr('disabled', false);
        });

        $("#selectAllSwitch").change(function () {
            if ($(this).is(":checked")) {
                isSelectAll = true;
                $(".choices").addClass('is-disabled');
                $('.choices__list').attr('hidden', true);
                $('.choices__input').attr('disabled', true);

                for (let i = 1; i <= allChoices.length; i++) {
                    $('#accountNumbers').append('<div class="col-sm-4 mb-2 flex-wrap"><input type="text" class="form-control" id=' + ("accNumber" + i) + ' disabled></div>');
                    $('#accNumber' + i).val(allChoices[i - 1]);
                }
            } else {
                isSelectAll = false;
                $('#accountNumbers').children('div').remove();
                $(".choices").removeClass('is-disabled');
                $('.choices__list').attr('hidden', false);
                $('.choices__input').attr('disabled', false);
            }
            validateInputs();
        });

        $('#secondNext').on('click', function () {
            Swal.fire({
                title: 'Do you want to submit the changes?',
                icon: 'warning',
                showCancelButton: true,
                confirmButtonText: 'Submit',
                allowOutsideClick: false,
                customClass: {
                    title: 'alertFontStyle'
                }
            }).then((result) => {
                if (result.isConfirmed) {
                    Swal.fire({
                        title: 'Account(s) converted to Statement Savings Account successfully!',
                        icon: 'success',
                        customClass: {
                            title: 'alertFontStyle'
                        }
                    }).then(function () {
                        $('#thirdPage').attr('hidden', false);
                        $('#secondPage').attr('hidden', true);
                        $('#firstPage').attr('hidden', true);
                    });
                }
            })

            // If the accounts were converted successfully
            $('#accountNumbers').children('div').each(function () {
                $('#successAccounts').append('<li>' + $(this).children('input').val() + '</li>');
            });

            // If the accounts were failed to convert
            // $('#accountNumbers').children('div').each(function () {
            //     $('#failedAccounts').append('<li>' + $(this).children('input').val() + '</li>');
            // });
        });

    });

    $('.reqPacBtn').on('click', function () {
        $(this).attr('disabled', true);
        $('.timerText').prop("hidden", false);
        startTimer(oneMin, display);
        Swal.fire(
            'OTP is Sent!',
            'OTP sent to your phone number 012 - * * * * 123',
            'success'
        )
    })

    var progressBar = $('.progress-bar');
    var progressNumber = 0;

    setInterval(function () {
        progressNumber++;
        progressBar.css('width', progressNumber + '%');
        progressBar.attr('aria-valuenow', progressNumber);
    }, 500);


</script>